/* proc print data=input; run; */

proc sql;
    select count(*) into: row_count
    from work.input;
run;
%put &=row_count.;

/* Part 1 */
%let hor_pos = 0;
%let depth = 0;

%macro part1(row_count);

    %do i = 1 %to &row_count.;
        
        data _null_;
            set work.input;
            if _n_ = &i. then do;
                if command = 'forward' then do;
                    call symputx('hor_pos',&hor_pos.+value);
                end;
                else if command = 'up' then do;
                    call symputx('depth',&depth.-value);                    
                end;
                else if command = 'down' then do;
                    call symputx('depth',&depth.+value);                    
                end;
            end;
        run;
        
    %end;
    
    %put &=hor_pos.;
    %put &=depth.;
    %put Multiply: %eval(&hor_pos. * &depth.);
    
%mend;

%part1(&row_count.);

/* Part 2 */
%let hor_pos = 0;
%let depth = 0;
%let aim = 0;

%macro part2(row_count);

    %do i = 1 %to &row_count.;
        
        data _null_;
            set work.input;
            if _n_ = &i. then do;
                if command = 'forward' then do;
                    call symputx('hor_pos',&hor_pos.+value);
                    call symputx('depth',&depth.+ (&aim. * value));  
                end;
                else if command = 'up' then do;
                    call symputx('aim',&aim.-value);                    
                end;
                else if command = 'down' then do;
                    call symputx('aim',&aim.+value);                    
                end;
            end;
        run;
        
    %end;
    
    %put &=hor_pos.;
    %put &=depth.;
    %put &=aim.;
    %put Multiply: %eval(&hor_pos. * &depth.);
    
%mend;

%part2(&row_count.);